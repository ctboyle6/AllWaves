
<div class="container">
  <h1 class="my-3">GO week view</h1>
  <div class="row justify-content-center py-3">
    <div class="col-10">
      <div class="card-group">
        <% i = 0 %>
        <% @go_array.each_slice(8) do |day| %>
          <div class="card">
            <strong><%= (Date.today + i).strftime("%A, %b %d") %></strong>
            <br>
            <%= p day %>
            <br>
            <br>
            <strong><%= day.include?(true) ? "GO" : "" %></strong>
          </div>
          <% i += 1 %>
          <br>
        <% end %>
      </div>
    </div>
  </div>
  <%= link_to "Preferences", preferences_path, class: "btn my-3"  %>
</div>




